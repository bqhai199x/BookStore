<div class="flex-w flex-l-m filter-tope-group m-tb-10" data-filter-group="cate">
    <button class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5 how-active1" data-filter="*">
        Tất cả thể loại
    </button>

    @foreach (var item in ViewBag.LstCategory)
    {
        string filterData = ".cate" + item.CategoryId;
        <button class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5" data-filter="@filterData">
            @item.Name
        </button>
    }
</div>


